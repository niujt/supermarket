<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.wxthxy.supermarket.dao.DeptDAO">
  <select id="deptlist" resultType="com.wxthxy.supermarket.entity.Dept">
    select *
    from smbms_dept
  </select>
  <select id="finddeptbyname" parameterType="String" resultType="com.wxthxy.supermarket.entity.Dept">
  	 select *
    from smbms_dept
    where dname=#{dname}
  </select>
</mapper>